{
  "name": "Latte With Crema",
  "type": "light",
  "colors": {
    "editor.background": "#fbf6ef",
    "editor.foreground": "#2b2321",
    "editorCursor.foreground": "#e98da9",
    "editor.lineHighlightBackground": "#f8f3eb",
    "editor.selectionBackground": "#e2d9cd",
    "editor.selectionHighlightBackground": "#f7eedfdd",
    "editorLineNumber.foreground": "#b5a89b",
    "editorIndentGuide.background1": "#f8f3eb",
    "editorIndentGuide.activeBackground1": "#e98da9",
    "activityBar.background": "#f1ece4",
    "activityBar.foreground": "#463c33",
    "titleBar.activeBackground": "#f7eedf",
    "titleBar.activeForeground": "#2b2321",
    "titleBar.inactiveBackground": "#f8f3eb",
    "titleBar.inactiveForeground": "#4a403c83",
    "statusBar.background": "#f1ece4",
    "statusBar.foreground": "#2b2321",
    "sideBar.background": "#f1ece4",
    "sideBar.foreground": "#3a302d",
    "sideBarTitle.foreground": "#2c1f1f",
    "list.activeSelectionBackground": "#e2d9cd",
    "list.activeSelectionForeground": "#2b2321",
    "button.background": "#e98da9",
    "button.foreground": "#2c1f1f",
    "scrollbarSlider.background": "#b5a89b",
    "tab.activeBackground": "#fbf6ef",
"tab.inactiveBackground": "#f8f3eb",
"tab.activeForeground": "#2b2321",
"tab.inactiveForeground": "#4a403c",
"editorGroup.border": "#b5a89b",
"sideBar.border": "#b5a89b",
"panel.border": "#b5a89b"

  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#4a403c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#b88e73"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#e98da9"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#e98da9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#3a302d"
      }
    },
    {
      "name": "Types / Classes",
      "scope": [
        "storage.type",
        "support.type",
        "entity.name.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#3a302d",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers / Constants",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant"
      ],
      "settings": {
        "foreground": "#84b49d"
      }
    },
    {
      "name": "Errors",
      "scope": [
        "invalid",
        "markup.error",
        "entity.name.exception"
      ],
      "settings": {
        "foreground": "#dc6257"
      }
    },
    {
      "name": "Warnings",
      "scope": [
        "markup.warning"
      ],
      "settings": {
        "foreground": "#d08e42"
      }
    },
    {
      "name": "Info / Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#84b49d"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "meta.tag",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#e98da9"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#b88e73"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#4a403c"
      }
    }
  ]
}